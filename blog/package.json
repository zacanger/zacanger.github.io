{
  "name": "blog",
  "description": "Zac Anger's blog.",
  "version": "1.0.0",
  "private": true,
  "author": "Zac Anger",
  "license": "LGPL-3.0",
  "main": "index.js",
  "scripts": {
    "build": "run-s build:webpack build:css build:rss",
    "build:css": "./build-css.sh",
    "build:rss": "node lib/rss.js",
    "build:webpack": "NODE_ENV=production webpack -p",
    "clean": "rm -f index.html && rm -f bundle.js && rm -rf posts && mkdir -p posts",
    "format": "prettier --write lib/**/*.js lib/*.js",
    "postbuild": "rm -f bundle.js",
    "prebuild": "sortpack && run-s clean test",
    "test": "npm run test:lint",
    "test:lint": "eslint -c .eslintrc.json lib/ webpack.config.babel.js"
  },
  "homepage": "http://zacanger/com/blog",
  "repository": {
    "type": "git",
    "url": "git@github.com/zacanger/blog.git"
  },
  "bugs": "https://github.com/zacanger/blog/issues",
  "keywords": [
    "blog",
    "design",
    "javascript",
    "js",
    "music",
    "programming",
    "vim",
    "web development",
    "writing"
  ],
  "dependencies": {
    "rss": "1.2.2"
  },
  "devDependencies": {
    "@babel/cli": "7.10.5",
    "@babel/core": "7.11.0",
    "@babel/plugin-proposal-class-properties": "7.10.4",
    "@babel/preset-env": "7.11.0",
    "@babel/preset-react": "7.10.4",
    "@babel/register": "7.10.5",
    "autoprefixer": "9.8.6",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "cheerio": "1.0.0-rc.3",
    "colors": "1.4.0",
    "cssnano": "4.1.10",
    "eslint": "7.6.0",
    "eslint-plugin-zacanger": "2.0.5",
    "front-matter": "4.0.2",
    "lodash": "4.17.19",
    "markdown-it": "11.0.0",
    "markdown-it-deflist": "2.0.3",
    "markdown-it-task-lists": "2.1.1",
    "moment": "2.27.0",
    "npm-run-all": "4.1.5",
    "postcss-cli-simple": "3.1.0",
    "prettier": "2.0.5",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "sortpack": "2.1.6",
    "static-site-generator-webpack-plugin": "3.4.2",
    "webpack": "3.12.0"
  }
}
