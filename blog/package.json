{
  "name": "blog",
  "description": "Zac Anger's blog.",
  "version": "1.0.0",
  "private": true,
  "author": "Zac Anger",
  "license": "LGPL-3.0",
  "main": "index.js",
  "scripts": {
    "build": "run-s build:webpack build:css build:rss",
    "build:css": "./build-css.sh",
    "build:rss": "node lib/rss.js",
    "build:webpack": "NODE_ENV=production webpack -p",
    "clean": "rm -f index.html && rm -f bundle.js && rm -rf posts && mkdir -p posts",
    "postbuild": "rm -f bundle.js",
    "prebuild": "sortpack && run-s clean test",
    "start": "webpack-dev-server --progress --colors",
    "test": "exit 0 || true || npm run test:lint",
    "test:lint": "eslint -c .eslintrc.json lib/ webpack.config.js"
  },
  "homepage": "http://zacanger/com/blog",
  "repository": {
    "type": "git",
    "url": "git@github.com/zacanger/blog.git"
  },
  "bugs": "https://github.com/zacanger/blog/issues",
  "keywords": [
    "blog",
    "design",
    "javascript",
    "js",
    "music",
    "programming",
    "vim",
    "web development",
    "writing"
  ],
  "dependencies": {
    "rss": "1.2.2"
  },
  "devDependencies": {
    "autoprefixer": "9.7.3",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "10.0.3",
    "babel-loader": "7.1.5",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-3": "6.24.1",
    "babel-register": "6.26.0",
    "cheerio": "1.0.0-rc.3",
    "colors": "1.4.0",
    "cssnano": "4.1.10",
    "eslint": "6.8.0",
    "eslint-config-zacanger": "3.4.3",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-import": "2.19.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-node": "11.0.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.17.0",
    "eslint-plugin-unicorn": "15.0.1",
    "front-matter": "3.0.2",
    "lodash": "4.17.15",
    "markdown-it": "10.0.0",
    "markdown-it-deflist": "2.0.3",
    "markdown-it-highlightjs": "3.0.0",
    "markdown-it-task-lists": "2.1.1",
    "moment": "2.24.0",
    "npm-run-all": "4.1.5",
    "postcss-cli-simple": "3.0.0",
    "prop-types": "15.7.2",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "sortpack": "2.0.4",
    "static-site-generator-webpack-plugin": "3.4.2",
    "webpack": "3.12.0",
    "webpack-dev-server": "3.10.1"
  }
}
